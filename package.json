{
  "name": "studiorack-app",
  "version": "0.0.16",
  "productName": "StudioRack",
  "description": "Audio plugin app, searchable list of plugins to install and share",
  "main": "main/index.js",
  "scripts": {
    "clean": "rimraf dist main renderer/out renderer/.next",
    "dev": "npm run build && electron .",
    "start": "electron .",
    "build-renderer": "next build renderer && next export renderer",
    "build-electron": "tsc -p electron-src",
    "build": "npm run build-renderer && npm run build-electron",
    "format": "prettier --write \"electron-src/**/*.ts\" \"renderer/pages/**/*.ts\"",
    "lint": "tslint -p electron-src/tsconfig.json",
    "pack-app": "npm run build && electron-builder --dir",
    "test": "echo 'No tests were run'",
    "dist": "npm run build && electron-builder --publish never",
    "type-check": "tsc"
  },
  "build": {
    "artifactName": "app-${os}.${ext}",
    "asar": true,
    "files": [
      "main",
      "renderer/out"
    ],
    "directories": {
      "buildResources": "renderer/out/icons"
    },
    "linux": {
      "executableName": "StudioRack",
      "artifactName": "app-${os}.${ext}"
    }
  },
  "author": "kmturley",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.4",
    "electron": "^10.1.2",
    "electron-builder": "^22.8.0",
    "next": "^9.5.1",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@studiorack/core": "0.0.40",
    "electron-is-dev": "^1.2.0",
    "electron-next": "^3.1.5",
    "electron-store": "^7.0.0",
    "gray-matter": "^4.0.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "remark": "^12.0.0",
    "remark-html": "^12.0.0",
    "slugify": "^1.4.5"
  }
}
